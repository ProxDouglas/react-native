{
  "name": "React Native Expo",
  "vers": 1,
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "ios": {
    "bundleIdentifier": "com.yourcompany.yourappname",
    "buildNumber": "1.0.0"
  },
  "android": {
    "package": "com.yourcompany.yourappname",
    "versionCode": 1
  },
  "scripts": {
    "lint": "eslint App.tsx app test --fix --ext .js,.ts,.tsx && npm run format",
    "bundle:visualize": "npx react-native-bundle-visualizer",
    "bundle:visualize:dev": "npx react-native-bundle-visualizer --dev",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "bundle:web": "npx expo export --platform web",
    "serve:web": "npx server dist",
    "prebuild:clean": "npx expo prebuild --clean",
    "prebuild": "npx expo prebuild",
    "loginExpo": "eas login",
    "build:prod": "eas build",
    "build:ios:prod": "eas build --platform ios",
    "build:android:prod": "eas build --platform android"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "^0.19.10",
    "expo-splash-screen": "~0.20.5",
    "expo-dev-client": "~2.4.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
